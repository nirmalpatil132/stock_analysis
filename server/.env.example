# server/.env.example
# Example environment variables.
# Copy this file to .env and fill in your actual credentials.

# Your PostgreSQL database connection URL
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://postgres:your_password@localhost:5432/stock_db"

# A secret key for signing JSON Web Tokens (JWT)
# Use a long, random string for security
JWT_SECRET="your_super_secret_jwt_key"

# Your free API key from Alpha Vantage (https://www.alphavantage.co/support/#api-key)
ALPHA_VANTAGE_API_KEY="YOUR_ALPHA_VANTAGE_API_KEY"